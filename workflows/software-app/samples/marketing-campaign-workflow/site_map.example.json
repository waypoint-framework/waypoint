    {
      "id": "campaign_creation",
      "title": "Create Campaign - CampaignAI",
      "url": "/campaigns/create",
      "type": "form",
      "template": "wizard_flow",
      "description": "Multi-step campaign creation wizard",
      "accessLevel": "authenticated",
      "parameters": [
        {
          "name": "step",
          "type": "query",
          "required": false,
          "description": "Current wizard step (1-5)",
          "validation": "Integer between 1-5"
        }
      ],
      "components": [
        {
          "id": "wizard_progress",
          "name": "Progress Indicator",
          "type": "navigation",
          "description": "Shows current step in campaign creation process",
          "position": "top",
          "dataElements": [
            {
              "field": "current_step",
              "type": "number",
              "source": "URL parameter",
              "format": "Step indicator"
            }
          ]
        },
        {
          "id": "campaign_goal_form",
          "name": "Campaign Goal Input",
          "type": "form",
          "description": "Step 1: User describes campaign objectives",
          "position": "middle",
          "interactiveElements": [
            {
              "id": "goal_input",
              "type": "textarea",
              "label": "What do you want to achieve?",
              "action": "AI analyzes input for suggestions",
              "validation": {
                "minLength": 10,
                "maxLength": 500
              },
              "states": ["empty", "typing", "analyzed", "error"]
            },
            {
              "id": "continue_btn",
              "type": "button",
              "label": "Continue",
              "action": "Navigate to next step",
              "states": ["disabled", "enabled", "loading"]
            }
          ]
        },
        {
          "id": "ai_generation_progress",
          "name": "AI Content Generation",
          "type": "modal",
          "description": "Shows AI progress while generating campaign",
          "position": "overlay",
          "dataElements": [
            {
              "field": "generation_progress",
              "type": "progress",
              "source": "AI generation API",
              "format": "Progress bar with status messages",
              "updateFrequency": "real_time"
            }
          ]
        }
      ],
      "userFlows": ["campaign_creation_flow"],
      "performance": {
        "loadPriority": "high",
        "lazyLoading": ["preview_images"],
        "preloading": ["wizard_step_2", "ai_models"]
      }
    },
